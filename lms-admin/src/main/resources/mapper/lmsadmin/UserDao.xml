<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.lmsadmin.dao.UserDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.example.lmsadmin.entity.UserEntity" id="userMap">
        <result property="userName" column="user_name"/>
        <result property="userId" column="user_id"/>
        <result property="password" column="password"/>
        <result property="gender" column="gender"/>
        <result property="phoneNumber" column="phone_number"/>
        <result property="borrowNumber" column="borrow_number"/>
        <result property="email" column="email"/>
        <result property="avatarPic" column="avatar_pic"/>
        <result property="balance" column="balance"/>
        <result property="remark" column="remark"/>
        <result property="birthdate" column="birthdate"/>
    </resultMap>


</mapper>